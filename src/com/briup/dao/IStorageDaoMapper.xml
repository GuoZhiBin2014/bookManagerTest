<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
			"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.briup.dao.IStorageDao">

	<insert id="addStorage" parameterType="Storage">
		<selectKey resultType="long" keyProperty="id" order="BEFORE">
			select user_seq.nextval from dual
		</selectKey>
		insert into
		t_storage(id,bookId,bookName,snumber,stime,operator)
		values(#{id},#{bookId},#{bookName},#{snumber},#{stime},#{operator}) 
	</insert>

	<select id="selectAll" resultType="Storage">
		select * from t_storage
	</select>
	
</mapper>
